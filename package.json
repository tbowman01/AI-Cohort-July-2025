{
  "name": "autodevhub",
  "version": "1.0.0-alpha",
  "description": "AI-Powered DevOps Story Generator - Transform development workflows with intelligent user story generation",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && python -m compileall .",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && pytest",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd frontend && npm run lint",
    "lint:backend": "cd backend && black . && isort . && flake8",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "clean": "docker-compose down -v && docker system prune -f"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/autodevhub.git"
  },
  "keywords": [
    "ai",
    "devops",
    "user-stories",
    "automation",
    "gherkin",
    "fastapi",
    "react",
    "development-tools"
  ],
  "author": "AI Cohort July 2025",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/autodevhub/issues"
  },
  "homepage": "https://github.com/yourusername/autodevhub#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "python": ">=3.9.0"
  },
  "dependencies": {
    "claude-flow": "^2.0.0-alpha.82",
    "init": "^0.1.2"
  }
}
